// простое инфоокно с заголовком
"infowindow": {
    "title": "name" // имя поля из которого берется подпись ИЛИ текст
  },


// простое инфоокно с подписью и текстовым описанием
"infowindow": {
    "title": "name", // имя поля из которого берется подпись ИЛИ текст
    "paragraph": "descriptio" // поле с текстовым описанием ИЛИ текст
  },


 // простое инфоокно с подписью и текстовым описанием
"infowindow": {
    "title": "name", // имя поля из которого берется подпись ИЛИ текст
    "paragraph": "descriptio" // поле с текстовым описанием ИЛИ текст
  }, 


// подпись, текстовый блок и доп инфа в виде телефона или ссылки
"infowindow": {
    "title": "name", // имя поля из которого берется подпись ИЛИ текст
    "paragraph": "descriptio", // поле с текстовым описанием ИЛИ текст
    "subscription": "phone" // поле с телефоном или сайтом (ссылка на сайт полная!)
  }, 

// подпись, текстовый блок и модальное окно
"infowindow": {
    "title": "name", // имя поля из которого берется подпись ИЛИ текст
    "paragraph": "descriptio", // поле с текстовым описанием ИЛИ текст
    "modal": true, 
    "modal_paragraph": "modal_text" // поле с текстом для модалки
  }, 

// инфоокно с подзаголовком
"infowindow": {
    "title": "name", // имя поля из которого берется подпись ИЛИ текст
    "subTitle": "какой-то подзаголовок" // имя поля из которого берется подпись ИЛИ текст
  },

// инфоокно с картикой
"infowindow": {
  "title": "name", // имя поля из которого берется подпись ИЛИ текст
  "image": "ccылка на картинку" // ссылка на картинку
},

// инфоокно со списком на основе атрибутивной таблицы
"infowindow": {
  "title": "name", // имя поля из которого берется подпись ИЛИ текст
  "list": [
    {"title":"имя поля в атрибутивке", "alias":"псевдоним, который отображается в инфоокне", "color":"hex для числа"}, // можно не указывать color, тогда число будет серым
    {"title":"имя поля в атрибутивке", "alias":"псевдоним, который отображается в инфоокне", "color":"hex для числа"},
    {"title":"имя поля в атрибутивке", "alias":"псевдоним, который отображается в инфоокне", "color":"hex для числа"},
  ]
},

